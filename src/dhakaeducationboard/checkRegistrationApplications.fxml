<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="593.0" prefWidth="723.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dhakaeducationboard.CheckRegistrationApplicationsController">
   <children>
      <Label layoutX="252.0" layoutY="40.0" text="Registration Applications">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="168.0" layoutY="521.0" mnemonicParsing="false" onMouseClicked="#sendApplicationsOnClick" prefHeight="38.0" prefWidth="387.0" text="Send Applications to Registration Office">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TableView fx:id="applicationTableView" layoutX="18.0" layoutY="151.0" prefHeight="342.0" prefWidth="666.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="177.0" text="Name" />
          <TableColumn fx:id="rollnumber" prefWidth="127.0" text="Roll Number" />
            <TableColumn fx:id="institute" prefWidth="188.0" text="Institution Name" />
            <TableColumn fx:id="session" prefWidth="173.0" text="Session" />
        </columns>
      </TableView>
      <Button layoutX="613.0" layoutY="28.0" mnemonicParsing="false" onMouseClicked="#logoutOnClick" text="logout" />
      <Label layoutX="18.0" layoutY="101.0" prefHeight="27.0" prefWidth="106.0" text="Examination">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="examComboBox" layoutX="142.0" layoutY="101.0" prefWidth="150.0" />
      <Button layoutX="325.0" layoutY="102.0" mnemonicParsing="false" onMouseClicked="#showApplicantsOnClick" text="Show Applicants" />
   </children>
</AnchorPane>
